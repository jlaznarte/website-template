  if (SDM.pf.dc.respObj.DynamicComponentsResp == undefined ||
      SDM.pf.dc.respObj.DynamicComponentsResp == null) {
    var linkRender = function() {
      clearInterval(retryIntrvl);
      if (DynamicArtContent.dynamicComponents == null || DynamicArtContent.dynamicComponents == undefined || retryCounter < retryNum) {
        retryCounter++;
        retryIntrvl = setInterval(linkRender, 500);
      } else {
        var outLinkHTML='';
        var oLinks=DynamicArtContent.dynamicComponents.OutwardLinks;
        if (oLinks!=undefined && oLinks.length>0) {
          for(i=0;i<oLinks.length;i++) {
            if(oLinks[i].LinkCat=='PermissionAndReprint') {
              outLinkHTML = DynamicArtContent.buildOutwardLinks(oLinks[i]);
            }
          }
        }
        if(outLinkHTML!='') {
          if($('.rightsLink').length) {
            $('.rightsLink').html(outLinkHTML);
            $('.extLinks').addClass('nonEmpty');
          } else {
            SDM.dac.rightsLink = outLinkHTML;
          }
        } else {
          $('.innerPaddingFixedBox .permissions').hide();
          $('.moreInformation .permissions').hide();
        }
      } 
    }
    var retryIntrvl = setInterval(linkRender, 0);
  } else {
    var outLinkHTML='';
