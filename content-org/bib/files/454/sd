<script type="text/javascript">
  var BI_LS_KEY = 'OX_BI',
      BI_QS = 'bi',
      urlPrefix = 'http://us-u.openx.net',
      path = '/w/1.0/sd',
      qa = 'cc=1&id=537072974&val=10301380622544627974',
      browserId = localStorage.getItem(BI_LS_KEY);

  if (browserId && qa) {
    var urlList = [ urlPrefix, path, '?', BI_QS, '=', browserId, '&', qa ];
    var url = urlList.join('');
    (new Image()).src = url;
  }
</script>
